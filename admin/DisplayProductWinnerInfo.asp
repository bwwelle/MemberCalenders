<%@LANGUAGE="VBSCRIPT" CODEPAGE="1252"%><!-- #include file ="dbconnections.asp" --><!-- #include file ="clphotosforvoting.asp" --><!-- #include file = "adminsession.asp" --><!-- #include virtual="/includes/dbconnections.inc" --><%Dim ProductIDDim rsWinningPhotosDim PhotoCountProductID = request.QueryString("productid")response.Write("Product ID = " & ProductID & "<BR>")set rsWinningPhotos = GetWinningPhotos(ProductID)PhotoCount = 0if not rsWinningPhotos.eof then	while not rsWinningPhotos.eof		PhotoCount = PhotoCount + 1		rsWinningPhotos.MoveNext	Wend	rsWinningPhotos.MoveFirstend if		OpenDatabase()%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" /><title>Winning Photos</title><LINK href="css/styles.css" rel="stylesheet" type="text/css"><link href="../css/style.css" rel="stylesheet" type="text/css" /></head><body><br /><table width="999" border="0" align="center" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF"> <!--header starts-->   <!--#include virtual="/header.asp"-->   <!--header ends-->  <tr>    <td colspan="2" align="left" valign="top">		<table width="100%" style="margin-bottom:5px;"> 	<tr>	<td width="136" class="leftnav" valign="top">		<!--left nav starts-->	<!--#include file="sidenav.asp"-->  	<!--Left Nav ends--> 	</td>  	<td width="12"  align="left" valign="top">&nbsp;</td>	<td width="848"  align="left" valign="top" bgcolor="#ffffff">    <!---------------------------Main Body --------------------------------------><% 	if not rsWinningPhotos.eof then %>	<table width="100%" cellpadding="2" cellspacing="0" border="0" bgcolor="#ffffff">		<tr>			<td>				<font style="font-size: large; text-decoration:underline; color: black;">All Round 3 Photos (sorted by number of 3rd round votes)</font>			</td>		</tr>		<tr>			<td valign="top">				<table width="100%" cellpadding="10" cellspacing="0" border="0">				<% 				Dim TotalRows				Dim rowCount					TotalRows = CLng(PhotoCount \ 5)				if PhotoCount mod 5 > 0 then					TotalRows = TotalRows + 1				end if						rowCount = 1				Dim rsVoteCountRoundOne				Dim rsVoteCountRoundTwo				Dim rsVoteCountRoundThree				Dim CurrentPhotoCount				Dim Round1PhotoCount				Dim Round2PhotoCount				Dim Round3PhotoCount				Dim PhotoID				Dim MasterPhotoID 				while not rsWinningPhotos.eof					PhotoID = rsWinningPhotos("ID")					ThumbNailPath = rsWinningPhotos("ThumbnailPath")					ThumbNailName = rsWinningPhotos("ThumbNailName")					MasterPhotoID = rsWinningPhotos("MasterPhotoID")					set rsVoteCountRoundOne = GetPhotoVoteCountByRound(MasterPhotoID, 1, MasterPhotoID)					Round1PhotoCount = rsVoteCountRoundOne("photocount")										set rsVoteCountRoundTwo = GetPhotoVoteCountByRound(MasterPhotoID, 2, 0)					Round2PhotoCount = rsVoteCountRoundTwo("photocount")						set rsVoteCountRoundThree = GetPhotoVoteCountByRound(PhotoID, 3, 0)					Round3PhotoCount = rsVoteCountRoundThree("photocount")										TotalVotes = Round1PhotoCount + Round2PhotoCount + Round3PhotoCount				%>				<% if rowCount = 1 then %>					<tr>				<% end if %>						<td style="width: 100px;">							<table width="100%" cellpadding="0" cellspacing="0" border="0" bordercolor="black">								<tr>									<td style="width: 100px;" colspan="2">										<a href="ShowImage.asp?pid=<%=ProductID%>&id=<%=rsWinningPhotos("id")%>&addcount=no" style="color: white; font-weight: bold; font-size: 10px; text-decoration:none;" target="_blank">																		<img src="../images/<%=ThumbNailPath%><%=ThumbNailName%>" border="1" width="150" height="100" style="border-color: black;"/></a>									</td>								</tr>								<tr>									<td style="background-color: #cccccc; height: 16px;" align="center">									 Round 1 Votes: <%=Round1PhotoCount%><br />									 Round 2 Votes: <%=Round2PhotoCount%><br />									 Round 3 Votes: <%=Round3PhotoCount%><br />									 Total Votes: <%=TotalVotes%><br />						                <%					Set SKU = DB.Execute("Select id,user from members where id=" & rsWinningPhotos("member_id") )					If not SKU.eof Then						USERNAME = SKU("user")					End If                %>								User: <%=USERNAME%><br />									 									 <br />									 <% 									 file_folder = Replace(ThumbNailPath,"thumbnails","images")									 file_name = Replace(ThumbNailName,"thumb_","")									 									 %>									 <a href="../images/<%=file_folder%><%=file_name%>">Download!</a>									</td>								</tr>							</table>						</td>						<% if rowCount = 5 then 							rowCount = 1						%>						</tr>						<% else 							rowCount = rowCount + 1					   end if 					rsWinningPhotos.MoveNext				Wend 				Dim i				if rowCount <= 5 then					For i = rowCount to 5 %>						<td colspan="2" style="width: 100px;"><br /></td>			<%		Next %>					</tr> <%				end if %>			</table>		</td>	</tr></table><% else %>	This product has not finished voting on the third round.<% end if %>	    <!-----------------------End Main Body -------------------------------------->    </td>	      </tr> </table></tr><!-- footer start --> <!--#include Virtual="/footer.asp"-->  <!-- footer end --></table><% 		CloseDatabase() %></body></html>	